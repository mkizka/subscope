{
  "name": "@repo/ingester",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "_eslint": "eslint --cache --cache-location ./node_modules/.cache/eslint",
    "_prettier": "prettier . --ignore-path ../../.gitignore --cache --log-level warn",
    "dev": "NODE_ENV=development tsx watch ./src/ingester.ts | pino-pretty",
    "format": "pnpm -s _eslint --fix && pnpm -s _prettier --ignore-unknown --write",
    "lint": "pnpm -s _eslint && pnpm -s _prettier --check",
    "start": "NODE_ENV=production tsx ./src/ingester.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@bull-board/api": "6.10.0",
    "@bull-board/express": "6.10.0",
    "@repo/common": "workspace:*",
    "@skyware/jetstream": "0.2.2",
    "express": "5.1.0",
    "typed-inject": "5.0.0",
    "ws": "8.18.2",
    "zod": "3.25.56",
    "zod-validation-error": "3.4.1"
  },
  "devDependencies": {
    "@keyv/redis": "4.5.0",
    "@mkizka/eslint-config": "7.0.1",
    "@tsconfig/node-lts": "22.0.1",
    "@types/express": "5.0.3",
    "@types/node": "22.15.30",
    "@types/ws": "8.18.1",
    "eslint": "9.28.0",
    "keyv": "5.3.4",
    "pino-pretty": "13.0.0",
    "tsx": "4.19.4",
    "typescript": "5.8.3"
  }
}
