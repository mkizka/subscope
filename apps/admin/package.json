    "@tailwindcss/vite": "^4.1.11",
    "@tsconfig/node-lts": "22.0.2",
    "@types/express": "5.0.3",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "eslint": "9.32.0",
