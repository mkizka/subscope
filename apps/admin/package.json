{
  "name": "@repo/admin",
  "type": "module",
  "private": true,
  "scripts": {
    "_eslint": "eslint --cache --cache-location ./node_modules/.cache/eslint",
    "_prettier": "prettier . --ignore-path ../../.gitignore --cache --log-level warn",
    "build": "react-router build",
    "dev": "NODE_ENV=development tsx node/server.ts",
    "format": "pnpm -s _eslint --fix && pnpm -s _prettier --ignore-unknown --write",
    "lint": "pnpm -s _eslint && pnpm -s _prettier --check",
    "start": "NODE_ENV=production tsx node/server.ts",
    "typecheck": "react-router typegen && tsc -b"
  },
  "dependencies": {
    "@bull-board/api": "^6.1.3",
    "@bull-board/express": "^6.1.3",
    "@react-router/express": "7.7.1",
    "@react-router/fs-routes": "7.7.1",
    "@react-router/node": "^7.7.1",
    "@repo/common": "workspace:*",
    "express": "5.1.0",
    "isbot": "^5.1.27",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.7.1",
    "typed-inject": "^4.0.0",
    "zod": "4.0.10",
    "zod-validation-error": "^4.0.0-beta.1"
  },
  "devDependencies": {
    "@mkizka/eslint-config": "7.1.1",
    "@react-router/dev": "^7.7.1",
    "@tailwindcss/vite": "^4.1.4",
    "@types/express": "5.0.3",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "eslint": "9.31.0",
    "prettier": "3.5.1",
    "tailwindcss": "^4.1.4",
    "tsx": "4.20.3",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
