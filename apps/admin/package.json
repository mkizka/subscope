    "@conform-to/react": "1.8.2",
    "@conform-to/zod": "1.8.2",
    "@react-router/express": "7.7.1",
    "@react-router/fs-routes": "7.7.1",
    "@react-router/node": "^7.7.1",
    "@repo/client": "workspace:*",
    "@repo/common": "workspace:*",
    "@repo/db": "workspace:*",
    "clsx": "2.1.1",
    "drizzle-orm": "0.44.4",
    "express": "5.1.0",
