{
  "name": "@repo/admin",
  "type": "module",
  "private": true,
  "scripts": {
    "_eslint": "eslint --cache --cache-location ./node_modules/.cache/eslint",
    "_prettier": "prettier . --ignore-path ../../.gitignore --cache --log-level warn",
    "_test:storybook:run": "wait-on tcp:9009 && test-storybook --url http://127.0.0.1:9009",
    "_test:storybook:server": "storybook dev -p 9009 --no-open > /dev/null 2>&1",
    "build": "react-router build",
    "build-storybook": "storybook build",
    "dev": "NODE_ENV=development tsx node/server.ts",
    "format": "pnpm -s _eslint --fix && pnpm -s _prettier --ignore-unknown --write",
    "lint": "react-router typegen && pnpm -s _eslint && pnpm -s _prettier --check",
    "start": "NODE_ENV=production tsx node/server.ts",
    "storybook": "storybook dev -p 6006",
    "test:storybook": "run-p -sr _test:storybook:*",
    "typecheck": "react-router typegen && tsc -b --noEmit"
  },
  "dependencies": {
    "@atproto/did": "0.2.3",
    "@atproto/jwk-jose": "^0.1.11",
    "@atproto/oauth-client-node": "^0.3.14",
    "@atproto/syntax": "0.4.2",
    "@atproto/xrpc": "0.7.7",
    "@bull-board/api": "^6.15.0",
    "@bull-board/express": "^6.15.0",
    "@conform-to/react": "1.14.1",
    "@conform-to/zod": "1.14.1",
    "@react-router/express": "7.10.1",
    "@react-router/fs-routes": "7.10.1",
    "@react-router/node": "^7.10.1",
    "@repo/client": "workspace:*",
    "@repo/common": "workspace:*",
    "@repo/db": "workspace:*",
    "@tanstack/react-query": "5.90.12",
    "clsx": "2.1.1",
    "drizzle-orm": "0.45.0",
    "express": "5.2.1",
    "isbot": "^5.1.32",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-router": "^7.10.1",
    "tailwind-merge": "3.4.0",
    "tsx": "4.21.0",
    "typed-inject": "^5.0.0",
    "zod": "4.1.13",
    "zod-validation-error": "^5.0.0"
  },
  "devDependencies": {
    "@iconify-json/tabler": "1.2.23",
    "@iconify/tailwind4": "1.2.0",
    "@mkizka/eslint-config": "7.2.0",
    "@playwright/test": "^1.57.0",
    "@react-router/dev": "^7.10.1",
    "@storybook/react-vite": "^10.1.4",
    "@storybook/test-runner": "^0.24.2",
    "@tailwindcss/vite": "^4.1.17",
    "@tsconfig/node-lts": "24.0.0",
    "@types/express": "5.0.6",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "daisyui": "5.5.8",
    "eslint": "9.39.1",
    "eslint-plugin-storybook": "^10.1.4",
    "npm-run-all": "4.1.5",
    "prettier": "3.7.4",
    "storybook": "^10.1.4",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "vite": "^7.2.6",
    "vite-tsconfig-paths": "^5.1.4",
    "wait-on": "9.0.3"
  }
}
