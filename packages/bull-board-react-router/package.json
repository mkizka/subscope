{
  "name": "@repo/bull-board-react-router",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "_eslint": "eslint --cache --cache-location ./node_modules/.cache/eslint",
    "_prettier": "prettier . --ignore-path ../../.gitignore --cache --log-level warn",
    "format": "pnpm -s _eslint --fix && pnpm -s _prettier --ignore-unknown --write",
    "lint": "pnpm -s _eslint && pnpm -s _prettier --check",
    "test:unit": "vitest run --project '*:unit'",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@bull-board/api": "6.16.4",
    "ejs": "3.1.10",
    "mime": "^4.1.0",
    "urlpattern-polyfill": "10.1.0"
  },
  "devDependencies": {
    "@mkizka/eslint-config": "8.0.0",
    "@repo/vitest": "workspace:*",
    "@tsconfig/node-lts": "24.0.0",
    "@types/ejs": "3.1.5",
    "@types/node": "25.1.0",
    "eslint": "9.39.2",
    "typescript": "5.9.3"
  },
  "sideEffects": false
}
