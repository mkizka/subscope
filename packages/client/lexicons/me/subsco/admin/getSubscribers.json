{
  "lexicon": 1,
  "id": "me.subsco.admin.getSubscribers",
  "defs": {
    "main": {
      "type": "query",
      "description": "Get a list of subscribers profiles.",
      "parameters": {
        "type": "params",
        "properties": {
          "limit": {
            "type": "integer",
            "minimum": 1,
            "maximum": 100,
            "default": 50
          },
          "cursor": {
            "type": "string"
          }
        }
      },
      "output": {
        "encoding": "application/json",
        "schema": {
          "type": "object",
          "required": ["subscribers"],
          "properties": {
            "cursor": {
              "type": "string"
            },
            "subscribers": {
              "type": "array",
              "items": {
                "type": "ref",
                "ref": "app.bsky.actor.defs#profileView"
              }
            }
          }
        }
      }
    }
  }
}
